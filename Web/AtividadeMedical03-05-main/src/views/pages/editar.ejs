

    
<div class="card col-md-12">
    <h5 class="card-header">Editar registro</h5>
    <div class="card-body">
      <h5 class="card-title"></h5>
      <!-- <p class="card-text">With supporting text below as a natural lead-in to additional content.</p> -->

      
    <form class="form-goup col-md-12" action="filme" method="post">
  
        <div class="container">
            <div class="form-floating mb-3 col-md-3">
                <input type="text" class="form-control-plaintext" id="id" name="id" value='<%= data[0].id %>' readonly>
                <label for="floatingInput">ID</label>
            </div>
        </div>
        <div class="container">
            <div class="form-floating mb-3 col-md-12">
                <input type="text" class="form-control " id="titulo" name="titulo" value='<%= data[0].titulo %>' placeholder="" required>
                <label for="titulo">Título</label>
            </div>
        </div>
        <div class="container">
            <div class="form-floating mb-3">
                <input type="number" minlength="4" maxlength="4" class="form-control" id="ano" name="ano" value='<%= data[0].ano %>' placeholder="" required>
                <label for="ano">Ano</label>
            </div>
        </div>

        <div class="container">
            <div class="d-grid gap-2 col-3">
                <button class="btn btn-primary col-md-12" type="submit">Salvar alterações</button>
            </div>
            <div class="d-grid gap-2 col-3">
                <a href="/listar" class="btn btn-secondary col-md-12">Cancelar</a>
            </div>
        </div>     

    </form>
    </div>
  </div>




